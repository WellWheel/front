                                    <tr>
                                        <th>Point de départ</th>
                                        <th>Point d'arrivé</th>
                                        <th>Paramètres</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% trajets.forEach(function(info) { %>
                                    <tr class="odd gradeX">
                                        <td><%= info.start %></td>
                                        <td><%= info.end %></td>
                                        <td class="center">
                                            <button type="button" class="btn btn-primary">Supprimer</button>
                                        </td>
                                    </tr>                                  
                                    <% }); %>                                 
                                </tbody>
                            </table>
        </div>

            </div>

</div>
